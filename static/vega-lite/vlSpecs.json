{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "Temperatur der letzten 7 Tage.",
  "width": 600,
  "height": 250,
  "data": {
    "name": "temperature"
  },
  "mark": "line",
  "encoding": {
    "x": {
      "field": "time",
      "timeUnit": "utcyearmonthdatehoursminutes",
      "type": "temporal",
      "scale": {
        "type": "utc"
      }
    },
    "y": {
      "field": "temperature",
      "type": "quantitative",
      "axis": {
        "title": "Temperatur"
      }
    }
  },
  "config": {}
}