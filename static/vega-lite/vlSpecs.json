{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "Temperatur der letzten 7 Tage.",
  "width": 600,
  "height": 250,
  "data": {
    "name": "temperature",
    "format": {
      "parse": {
        "time": "utc:'%Y-%m-%d %H:%M:%S'"
      }
    }
  },
  "mark": "line",
  "encoding": {
    "x": {
      "field": "time",
      "timeUnit": "utcyearmonthdatehoursminutesseconds",
      "type": "temporal",
      "scale": {
        "type": "utc"
      },
      "axis": {
        "labelAngle": 15,
        "title": "Zeit"
      }
    },
    "y": {
      "field": "temperature",
      "type": "quantitative",
      "axis": {
        "title": "Temperatur"
      }
    }
  },
  "config": {}
}